<?xml version="1.0" encoding="UTF-8"?>

    <!--媒体平台相关配置-->
<config xsi:noNamespaceSchemaLocation="SDKSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <!--缓存队列设置-->
    <offlineCache>
        <length>0</length>
        <queueExpirationSecs>60</queueExpirationSecs>
         <!--发送超时时间-->
        <timeout>60</timeout>
    </offlineCache>

    <companies>
        <company>
            <name>admaster</name>
            <domain>
                <!-- 此处需修改为第三方检测公司监测代码的 host 部分 -->
                <url>admaster.com.cn</url>
            </domain>
            <signature>
                <publicKey>Z83476Hel</publicKey>
                <paramKey>sign</paramKey>
            </signature>
            <switch>
                <isTrackLocation>false</isTrackLocation>
                <!-- 失效时间，单位秒 -->
                <offlineCacheExpiration>259200</offlineCacheExpiration>
                <encrypt>
                    <MAC>md5</MAC>
                    <IDA>md5</IDA>
                    <IMEI>md5</IMEI>
                    <ANDROIDID>raw</ANDROIDID>
                </encrypt>
            </switch>
            <config>
                <arguments>
                    <!--argument的必选和常用可选参数 key需确定-->
                    <!--必选函数-->
                    <argument>
                        <key>OS</key>
                        <value>0a</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>TS</key>
                        <value>t</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>MAC</key>
                        <value>n</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>IDFA</key>
                        <value>z</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>IMEI</key>
                        <value>0c</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>ANDROIDID</key>
                        <value>0d</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>WIFI</key>
                        <value>w</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>AKEY</key>
                        <value>x</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>ANAME</key>
                        <value>y</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <!--可选函数-->
                    <argument>
                        <key>SCWH</key>
                        <value>0f</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>OPENUDID</key>
                        <value>o</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>ODIN</key>
                        <value>0g</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>TERM</key>
                        <value>r</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>OSVS</key>
                        <value>q</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>

                    <argument>
                        <key>LBS</key>
                        <value>l</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>MUID</key>
                        <value>0h</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>MUDS</key>
                        <value>0i</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>REDIRECTURL</key>
                        <value>u</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                </arguments>
                <events>
                    <event>
                        <!--<name>m1</name>-->
                        <key>start</key>
                        <value>m201</value>
                        <urlEncode>true</urlEncode>
                    </event>
                    <event>
                        <!--<name>e1</name>-->
                        <key>end</key>
                        <value>m203</value>
                        <urlEncode>true</urlEncode>
                    </event>

                </events>


            </config>
            <separator>,</separator>
            <!--<separator>&amp;</separator>-->
            <equalizer></equalizer>
            <!--如果设置true   timeStamper使用秒-->
            <timeStampUseSecond>true</timeStampUseSecond>
        </company>

        <company>
            <name>miaozhen</name>
            <domain>
                <url>miaozhen.com</url>
            </domain>
            <signature>
                <publicKey>DbXiUlEVN</publicKey>
                <paramKey>mf</paramKey>
            </signature>
            <switch>
                <isTrackLocation>true</isTrackLocation>
                <offlineCacheExpiration>604800</offlineCacheExpiration>
                <encrypt>
                    <MAC>raw</MAC>
                    <IDA>raw</IDA>
                    <IMEI>raw</IMEI>
                    <ANDROIDID>raw</ANDROIDID>
                </encrypt>
            </switch>      
            <config>
                <arguments>
                    <argument>
                        <key>OS</key>
                        <value>mo</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>TS</key>
                        <value>mt</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>MAC</key>
                        <value>m7</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>IDFA</key>
                        <value>m5</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>IMEI</key>
                        <value>m3</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>ANDROIDID</key>
                        <value>m1</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>TERM</key>
                        <value>md</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>WIFI</key>
                        <value>mk</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>AKEY</key>
                        <value>mp</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>ANAME</key>
                        <value>mn</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>SCWH</key>
                        <value>mh</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>OPENUDID</key>
                        <value>m0</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>OSVS</key>
                        <value>me</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>LBS</key>
                        <value>mm</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>true</isRequired>
                    </argument>
                    <argument>
                        <key>REDIRECTURL</key>
                        <value>o</value>
                        <urlEncode>true</urlEncode>
                        <isRequired>false</isRequired>
                    </argument>
                </arguments>
                <events>
                    <event>
                        <key>start</key>
                        <value>mb=start</value>
                        <urlEncode>false</urlEncode>
                    </event>
                    <event>
                        <key>end</key>
                        <value>mb=end</value>
                        <urlEncode>false</urlEncode>
                    </event>
                </events>
            </config>    
            <separator>&amp;</separator>         
            <equalizer>=</equalizer>
            <timeStampUseSecond>true</timeStampUseSecond>
        </company>

    </companies>
</config>